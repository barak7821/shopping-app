{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all --parallel install:client install:server install:admin",
    "install:client": "cd frontend && npm install",
    "install:server": "cd backend && npm install",
    "install:admin": "cd admin && npm install",
    "dev": "concurrently -n CLIENT,SERVER,ADMIN -c cyan,green,blue \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:admin\"",
    "dev:client": "cd frontend && npm run dev",
    "dev:server": "cd backend && npm run dev",
    "dev:admin": "cd admin && npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "dependencies": {
    "concurrently": "^9.1.2",
    "express": "^5.1.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
